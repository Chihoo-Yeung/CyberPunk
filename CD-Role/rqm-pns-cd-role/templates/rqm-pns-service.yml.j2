apiVersion: v1
kind: Service
metadata:
  name: "{{ SvcName }}"
  namespace: {{ Namespace }}
  labels:
    spp-app: "spp-{{ AppName }}"
spec:
  type: NodePort
  selector:
    spp-app: "spp-{{ AppName }}"
  ports:
  - name: port01
    nodePort: 8088
    port: 8088
    targetPort: 8088
    protocol: TCP
  - name: port02
    nodePort: 9088
    port: 9088
    targetPort: 9088
    protocol: TCP
